{
  "name": "@hanghae-plus/shopping",
  "version": "0.0.1",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp ./dist/index.html ./dist/404.html",
    "tsc": "tsc --noEmit",
    "lint:fix": "eslint --fix",
    "prettier:write": "prettier --write ./src",
    "preview": "vite preview",
    "build:prod": "NODE_ENV=production vite build",
    "preview:prod": "NODE_ENV=production vite preview",
    "deploy": "pnpm build:prod && pnpm preview:prod"
  },
  "dependencies": {
    "react": "latest",
    "react-dom": "latest",
    "@hanghae-plus/lib": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-transform-react-jsx": "latest",
    "@eslint/js": "^9.16.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/use-sync-external-store": "latest",
    "@vitejs/plugin-react-oxc": "latest",
    "@types/node": "^24.0.13",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "msw": "^2.10.2",
    "prettier": "^3.4.2",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest"
  }
}
